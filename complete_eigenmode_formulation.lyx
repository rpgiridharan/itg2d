\]

\end_inset


\begin_inset Formula 
\[
\frac{\partial}{\partial t}\xi_{k}^{\pm}+i\omega_{k}^{\pm}\xi_{k}^{\pm}=\sum_{p+q=-k}\hat{\mathbf{z}}\times\mathbf{p}\cdot\mathbf{q}\left(\Phi_{p}^{*}\left(\zeta_{q}^{*}-\frac{i\omega_{k}^{\pm}}{C}q^{2}\Phi_{q}^{*}\right)-\Phi_{q}^{*}\left(\zeta_{p}^{*}-\frac{i\omega_{k}^{\pm}}{C}p^{2}\Phi_{p}^{*}\right)\right)
\]

\end_inset


\begin_inset Formula 
\[
