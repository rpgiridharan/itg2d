\begin_layout Standard
consider
\begin_inset Formula 
\[
\frac{\partial}{\partial t}\zeta+\hat{\mathbf{z}}\times\nabla\Phi\cdot\nabla\zeta+\kappa\partial_{y}\Phi=0
\]

\end_inset

and
\begin_inset Formula 
\[
\frac{\partial}{\partial t}\nabla^{2}\Phi+\hat{\mathbf{z}}\times\nabla\Phi\cdot\nabla\nabla^{2}\Phi=C\left(\widetilde{\Phi}-\nabla^{2}\widetilde{\Phi}-\zeta\right)
\]

\end_inset


\begin_inset Formula 
\[
\frac{\partial}{\partial t}\zeta_{k}+i\kappa k_{y}\Phi_{k}=0
\]

\end_inset


\begin_inset Formula 
\[
k^{2}\frac{\partial}{\partial t}\Phi_{k}=C\left(\zeta_{k}-\widetilde{\Phi}_{k}-k^{2}\widetilde{\Phi}_{k}\right)
\]

\end_inset

consider 
\begin_inset Formula $\xi_{k}^{\pm}=\zeta_{k}+a_{k}^{\pm}k^{2}\Phi_{k}$
\end_inset


\begin_inset Formula 
\[
\frac{\partial}{\partial t}\left(\zeta_{k}+a_{k}^{\pm}k^{2}\Phi_{k}\right)=-i\kappa k_{y}\Phi_{k}+Ca_{k}^{\pm}\left(\zeta_{k}-\widetilde{\Phi}_{k}-k^{2}\widetilde{\Phi}_{k}\right)=-i\omega_{k}^{\pm}\left(\zeta_{k}+a_{k}^{\pm}k^{2}\Phi_{k}\right)
\]

\end_inset

which means:
\begin_inset Formula 
\[
-i\kappa k_{y}\Phi_{k}+Ca_{k}^{\pm}\left(\zeta_{k}-\widetilde{\Phi}_{k}\left(1+k^{2}\right)\right)=-i\omega_{k}^{\pm}\left(\zeta_{k}+a_{k}^{\pm}k^{2}\Phi_{k}\right)
\]

\end_inset


\begin_inset Formula 
\[
a_{k}^{\pm}=-\frac{i\omega_{k}^{\pm}}{C}
\]

\end_inset

and
\begin_inset Formula 
\[
-i\kappa k_{y}-Ca_{k}^{\pm}\left(1+k^{2}\right)=-i\omega_{k}^{\pm}a_{k}^{\pm}k^{2}
\]

\end_inset

which gives the dispersion relation:
\begin_inset Formula 
\[
\omega_{k}^{\pm2}+i\frac{C}{k^{2}}\omega_{k}^{\pm}\left(1+k^{2}\right)=i\frac{C}{k^{2}}\kappa k_{y}
\]

\end_inset

note that 
\begin_inset Formula 
\[
\omega_{k}^{+}+\omega_{k}^{-}=-i\frac{C}{k^{2}}\left(1+k^{2}\right)
\]

\end_inset

which means
\begin_inset Formula 
\[
\omega_{k}^{-}=-\omega_{k}^{+}-i\frac{C}{k^{2}}\left(1+k^{2}\right)
\]

\end_inset


\begin_inset Formula 
\[
\boxed{\xi_{k}^{\pm}=\zeta_{k}-\frac{i\omega_{k}^{\pm}}{C}k^{2}\Phi_{k}}
\]

\end_inset


\begin_inset Formula 
\[
\boxed{\Phi_{k}\equiv i\frac{C\left(\xi_{k}^{+}-\xi_{k}^{-}\right)}{k^{2}\left(\omega_{k}^{+}-\omega_{k}^{-}\right)}=i\frac{C\left(\xi_{k}^{+}-\xi_{k}^{-}\right)}{2k^{2}W_{k}}}
\]

\end_inset

noting that:
\begin_inset Formula 
\[
\boxed{\omega_{k}^{\pm}=-i\frac{C}{2k^{2}}\left(1+k^{2}\right)\pm W_{k}}
\]

\end_inset

which makes 
\begin_inset Formula 
\[
\omega_{k}^{+}-\omega_{k}^{-}=2W_{k}
\]

\end_inset

with 
\begin_inset Formula $W_{k}$
\end_inset

 still complex.
\begin_inset Formula 
\[
\boxed{\zeta_{k}=\frac{\omega_{k}^{-}\xi_{k}^{+}-\omega_{k}^{+}\xi_{k}^{-}}{\left(\omega_{k}^{-}-\omega_{k}^{+}\right)}=\frac{\omega_{k}^{+}\xi_{k}^{-}-\omega_{k}^{-}\xi_{k}^{+}}{2W_{k}}}
\]

\end_inset


\begin_inset Formula 
\[
